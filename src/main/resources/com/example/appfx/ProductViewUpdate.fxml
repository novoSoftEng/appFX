<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.productViewUpdate">
    <children>
        <Label alignment="CENTER" prefHeight="53.0" prefWidth="241.0" text="Modifier le Produit " />
        <VBox layoutX="88.0" layoutY="61.0" prefHeight="331.0" prefWidth="425.0" alignment="CENTER">
            <children>
                <Label text="Nom Produit" />
                <TextField fx:id="nom" />
                <Label text="Fornisseur :" />
                <TextField fx:id="fornisseur" />

            </children>
            <Button fx:id="ok" onAction="#onUpdateButtonClick" text="Save!" />
        </VBox>

    </children>

</AnchorPane>
